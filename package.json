{
  "name": "gishohaku-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next app",
    "build:app": "next build app",
    "export:app": "next export app --outdir dist/public",
    "start": "next start app",
    "copy:build:app": "cp -a dist/functions/next/static dist/public/_next",
    "build:functions": "babel functions -d dist/functions --extensions='.ts'",
    "copy:deps": "cp package.json dist/functions; cp package-lock.json dist/functions",
    "predeploy": "yarn build:app && yarn build:functions && yarn copy:build:app && yarn export:app && yarn copy:deps",
    "deploy": "firebase deploy",
    "functions:shell": "firebase experimental:functions:shell",
    "functions:emulator": "yarn firebase emulators:start --only functions",
    "buckup:firestore": "gcloud beta firestore export gs://gishohaku.appspot.com",
    "runscript": "babel-node --extensions='.tsx,.ts'"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/css": "^10.0.9",
    "@emotion/styled": "^10.0.23",
    "@google-cloud/pubsub": "^1.1.6",
    "@google-cloud/storage": "^4.1.3",
    "@mdx-js/loader": "^1.3.1",
    "@next/mdx": "^9.0.4",
    "@sentry/browser": "^5.4.3",
    "@slack/web-api": "^5.6.0",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.0",
    "babel-plugin-emotion": "^10.0.16",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.15",
    "firebase": "^6.4.0",
    "firebase-admin": "^8.4.0",
    "firebase-functions": "^3.3.0",
    "firebase-tools": "^7.2.4",
    "formik": "^1.5.4",
    "immer": "^3.1.3",
    "marked": "^0.6.2",
    "next": "^9.2.0",
    "next-images": "^1.1.1",
    "qs": "^6.7.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-dropzone": "^10.1.4",
    "react-ga": "^2.6.0",
    "react-helmet": "^5.2.1",
    "react-image-lightbox": "^5.1.0",
    "react-infinite-scroller": "^1.2.4",
    "react-lazyload": "^2.5.0",
    "react-virtualized": "^9.21.1",
