    "babel-plugin-emotion": "^10.0.9",
    "classnames": "^2.2.6",
    "firebase": "^5.10.1",
    "firebase-admin": "^7.3.0",
    "firebase-functions": "^2.3.0",
    "next": "^8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sancho": "^1.2.3"
